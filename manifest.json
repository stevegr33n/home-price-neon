{
    "name": "Rightmove Enhancement Suite",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "description": "Enables you to see a property's price history data on Rightmove.co.uk",
    "content_scripts": [{
        "matches": [
            "https://www.rightmove.co.uk/*"
        ],
        "css": [
            "rightmove-enhancement-suite.css"
        ],
        "js": [
            "content-script.js"
        ]
    }],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "images/icons/icon16.png",
        "32": "images/icons/icon32.png",
        "48": "images/icons/icon48.png",
        "128": "images/icons/icon128.png"
    },
    "permissions": ["storage", "tabs"],
    "host_permissions":["https://rub-a-dub-dub.club/create-user", "https://rub-a-dub-dub.club/get-property-prices"]
}
